<ibst>
   <!-- "settings" node is not part of the binary -->
   <settings>
      <rsa_key name="ftpr_key" value="IBST_Zephyr/priv.pem" />
      <rsa_key name="key" value="IBST_Zephyr/privkey_2k.pem" hash_type="sha512" />
      <number name="ftpr_svn" value="0" />
      <version name="ftpr_version" value="0.0.0.0" />
      <elf_file name="rbe_elf" value="IBST_Zephyr/rbe.elf">
         <module_entry value="ModuleEntry" />
         <objcopy_args value="-O binary --change-section-lma .data=0x4b000" />
      </elf_file>
      
      <elf_file name="grtos_elf" value="IBST_Zephyr/zephyr.elf">
         <module_entry value="main" />
      </elf_file>
      <number name="grtos_enabled" value="1" />
      
      <!--<elf_file name="rtos_elf" value="IBST_Zephyr/rtos.elf">
         <module_entry value="ModuleEntry" />
      </elf_file>-->
      <number name="rtos_enabled" value="0" />
      
      <!--<file name="data" value="IBST_Zephyr/data.bin" >
         <offset value="0" />
         <end value="0x1000" />
      </file>-->
      <number name="data_enabled" value="0" />
      
      <date name="date" />

   </settings>

   <!-- "layout" node describes structure of the binary -->
   <layout>
      <rom_bypass>
         <byte_array name="reserved" size="16" value="0" />
      </rom_bypass>
      <fpt>
         <fpt_header>
            <string name="header_markup" size="4" value="$FPT" />
            <number name="number_of_entries" size="4" calculate="parent/parent/entries.size / parent/parent/entries/fpt_entry.size" />
            <number name="header_version" size="1" value="0x20" />
            <number name="entry_version" size="1" value="0x10" />
            <number name="header_length" size="1" value="0x20" />
            <function_checksum name="header_checksum">
               <input>
                  <data start="parent.offset" end="this.offset" />
                  <data path="parent/reserved" />
               </input>
            </function_checksum>
            <byte_array name="reserved" size="20" value="0" />
         </fpt_header>
         <entries>
            <fpt_entry>
               <string name="partition_name" size="4" value="FTPR" />
               <number name="reserved" size="4" value="0" />
               <number name="offset" size="4" calculate="/layout/ftpr.offset" />
               <number name="length" size="4" calculate="/layout/ftpr.size"/>
               <number name="reserved" size="12" value="0" />
               <number name="partition_flags" size="4" value="0" />
            </fpt_entry>
            <fpt_entry>
               <string name="partition_name" size="4" value="OKM" />
               <number name="reserved" size="4" value="0" />
               <number name="offset" size="4" calculate="/layout/okm.offset" />
               <number name="length" size="4" calculate="/layout/okm.size"/>
               <number name="reserved" size="12" value="0" />
               <number name="partition_flags" size="4" value="0" />
            </fpt_entry>
         </entries>
      </fpt>

      <!-- FTPR partition -->
      <partition name="ftpr" align="0x1000">
         <pdt>
            <pdt_header>
               <string name="header_marker" size="4" value="$CPD" />
               <number name="number_of_entries" size="4" calculate="parent/parent/entries.size / parent/parent/entries/pdt_entry.size" />
               <number name="header_version" size="1" value="1" />
               <number name="entry_version" size="1" value="1" />
               <number name="header_length" size="1" value="0x10" />
               <number name="reserved" size="1" value="0" />
               <string name="partition_name" size="4" value="FTPR" />
            </pdt_header>
            <entries>
               <pdt_entry>
                  <string name="entry_name" size="12" value="FTPR.man" />
                  <number name="offset" size="4" calculate="parent/parent/parent/parent/manifest.offset - parent/parent/parent/parent.offset" />
                  <number name="length" size="4" calculate="parent/parent/parent/parent/manifest.size" />
                  <number name="reserved" size="4" value="0" />
               </pdt_entry>
               <pdt_entry>
                  <string name="entry_name" size="12" value="rbe" />
                  <number name="offset" size="4" calculate="parent/parent/parent/parent/rbe.offset - parent/parent/parent/parent.offset" />
                  <number name="length" size="4" calculate="parent/parent/parent/parent/rbe.size" />
                  <number name="reserved" size="4" value="0" />
               </pdt_entry>
               <pdt_entry>
                  <string name="entry_name" size="12" value="rbe.met" />
                  <number name="offset" size="4" calculate="parent/parent/parent/parent/rbe_met.offset - parent/parent/parent/parent.offset" />
                  <number name="length" size="4" calculate="parent/parent/parent/parent/rbe_met.size" />
                  <number name="reserved" size="4" value="0" />
               </pdt_entry>
               <pdt_entry>
                  <string name="entry_name" size="12" value="grtos" />
                  <number name="offset" size="4" calculate="parent/parent/parent/parent/grtos.offset - parent/parent/parent/parent.offset" />
                  <number name="length" size="4" calculate="parent/parent/parent/parent/grtos.size" />
                  <number name="reserved" size="4" value="0" />
               </pdt_entry>
               <pdt_entry>
                  <string name="entry_name" size="12" value="grtos.met" />
                  <number name="offset" size="4" calculate="parent/parent/parent/parent/grtos_met.offset - parent/parent/parent/parent.offset" />
                  <number name="length" size="4" calculate="parent/parent/parent/parent/grtos_met.size" />
                  <number name="reserved" size="4" value="0" />
               </pdt_entry>
               <pdt_entry>
                  <string name="entry_name" size="12" value="rtos" />
                  <number name="offset" size="4" calculate="parent/parent/parent/parent/rtos.enabled ? parent/parent/parent/parent/rtos.offset - parent/parent/parent/parent.offset : 0" />
                  <number name="length" size="4" calculate="parent/parent/parent/parent/rtos.size" />
                  <number name="reserved" size="4" value="0" />
               </pdt_entry>
               <pdt_entry>
                  <string name="entry_name" size="12" value="rtos.met" />
                  <number name="offset" size="4" calculate="parent/parent/parent/parent/rtos_met.offset - parent/parent/parent/parent.offset" />
                  <number name="length" size="4" calculate="parent/parent/parent/parent/rtos_met.size" />
                  <number name="reserved" size="4" value="0" />
               </pdt_entry>
               <pdt_entry>
                  <string name="entry_name" size="12" value="data" />
                  <number name="offset" size="4" calculate="parent/parent/parent/parent/data.enabled ? parent/parent/parent/parent/data.offset - parent/parent/parent/parent.offset : 0" />
                  <number name="length" size="4" calculate="parent/parent/parent/parent/data.size" />
                  <number name="reserved" size="4" value="0" />
               </pdt_entry>
               <pdt_entry>
                  <string name="entry_name" size="12" value="data.met" />
                  <number name="offset" size="4" calculate="parent/parent/parent/parent/data.enabled ? parent/parent/parent/parent/data_met.offset - parent/parent/parent/parent.offset : 0" />
                  <number name="length" size="4" calculate="parent/parent/parent/parent/data_met.size" />
                  <number name="reserved" size="4" value="0" />
               </pdt_entry>
            </entries>
         </pdt>

         <manifest  align="0x1000"> <!--  ftpr manifest-->
            <manifest_header>
               <number name="header_type" size="4" value="4" />
               <number name="header_length" size="4" calculate="parent.size / 4" />
               <number name="header_version" size="4" value="0x10000" />
               <bit_field name="flags" size="4">
                  <bit name="pv_state" bit_low="0" bit_high="0" value="0" />
                  <bit name="reserved" bit_low="1" bit_high="30" value="0" />
                  <bit name="flags_debug" bit_low="31" bit_high="31" value="0"/>
               </bit_field>
               <number name="vendor" size="4" value="0x8086" />
               <byte_array name="date" size="4" calculate="/settings/date.bcd" />
               <number name="size" size="4" calculate="parent/parent.size / 4" />
               <string name="header_id" size="4" value="$MN2" />
               <number name="reserved" size="4" value="0" />
               <number name="version" size="8" calculate="/settings/ftpr_version.value" />
               <number name="svn" size="4" calculate="/settings/ftpr_svn.value" />
               <number name="reserved" size="72" value="0" />
               <number name="modulus_size" size="4" calculate="parent/modulus.size / 4" />
               <number name="exponent_size" size="4" calculate="parent/exponent.size / 4" />
               <byte_array name="modulus" calculate="/settings/ftpr_key.modulus" /> <!-- "size" attribute is taken from calculate (size of the result) -->
               <byte_array name="exponent" calculate="/settings/ftpr_key.exponent" />
               <function_sign name="signature">
                  <key value="/settings/ftpr_key" />
                  <sha value="256" />
                  <padding_scheme value="v1_5" salt_len=""/>
                  <input>
                     <data start="parent.offset" end="parent/modulus.offset" />
                     <data path="parent/parent/partition_info_extension" />
                  </input>
               </function_sign>
            </manifest_header>
            <partition_info_extension>
               <partition_info_header>
                  <number name="extension_type" size="4" value="3" />
                  <number name="extension_length" size="4" calculate="parent/parent.size" />
                  <string name="partition_name" size="4" calculate="parent/parent/parent/parent/pdt/pdt_header/partition_name.value" />
                  <number name="partition_length" size="4" value="0" />
                  <byte_array name="partition_hash" size="32" value="0" />
                  <number name="version_control_number" size="4" value="0" />
                  <number name="partition_version" size="4" value="0" />
                  <number name="data_format_version" size="4" value="0" />
                  <number name="instance_id" size="4" value="0" />
                  <number name="flags" size="4" value="0" />
                  <byte_array name="reserved" size="20" value="0" />
               </partition_info_header>
               <partition_info_entries>
                  <entry>
                     <string name="name" size="12" value="rbe" />
                     <number name="type" size="4" value="0" />
                     <number name="metadata_size" size="4" calculate="parent/parent/parent/parent/parent/rbe_met.size" />
                     <function_hash name="metadata_hash">
                        <sha value="256" />
                        <input>
                           <data path="parent/parent/parent/parent/parent/rbe_met" />
                        </input>
                     </function_hash>
                  </entry>
               </partition_info_entries>
            </partition_info_extension>
         </manifest>

         <metadata name="rbe_met">
            <module_attribute_extension>
               <number name="extension_type" size="4" value="10" />
               <number name="extension_length" size="4" calculate="parent.size" />
               <number name="compression_type" size="1" value="0" />
               <number name="encryption_type" size="1" value="0" />
               <number name="mode" size="1" value="0" />
               <number name="reserved" size="1" value="0" />
               <number name="uncompressed_size" size="4" calculate="parent/parent/parent/rbe.size" />
               <number name="compressed_size" size="4" calculate="parent/parent/parent/rbe.size" />
               <number name="global_module_id_process_number" size="2" value="1" />
               <number name="global_module_id_vendor_id" size="2" value="0x8086" />
               <function_hash name="image_hash">
                  <sha value="256" />
                  <input>
                     <data path="parent/parent/parent/rbe" />
                  </input>
               </function_hash>
            </module_attribute_extension>
            <process_attribute_extension_cdf>
               <number name="extension_type" size="4" value="5" />
               <number name="extension_length" size="4" calculate="parent.size" />
               <number name="process_flags" size="4" value="0" />
               <number name="main_thread_id" size="4" value="0" />
               <number name="private_code_base_address" size="4" calculate="/settings/rbe_elf.private_code_base_address" />
               <number name="uncompressed_private_code_size" size="4" calculate="/settings/rbe_elf.uncompressed_private_code_size" />
               <number name="cm0_heap_size" size="4" value="0" />
               <number name="bss_size" size="4" calculate="/settings/rbe_elf.bss_size" />
               <number name="def_heap_size" size="4" value="0" />
               <number name="main_thread_entry_point" size="4" calculate="/settings/rbe_elf.entry_address" />
               <number name="reserved" size="28" value="0" />
            </process_attribute_extension_cdf>
         </metadata>
         
         <metadata name="grtos_met">
            <module_attribute_extension>
               <number name="extension_type" size="4" value="10" />
               <number name="extension_length" size="4" calculate="parent.size" />
               <number name="compression_type" size="1" value="0" />
               <number name="encryption_type" size="1" value="0" />
               <number name="mode" size="1" value="0" />
               <number name="reserved" size="1" value="0" />
               <number name="uncompressed_size" size="4" calculate="parent/parent/parent/grtos.size" />
               <number name="compressed_size" size="4" calculate="parent/parent/parent/grtos.size" />
               <number name="global_module_id_process_number" size="2" value="1" />
               <number name="global_module_id_vendor_id" size="2" value="0x8086" />
               <function_hash name="image_hash">
                  <sha value="256" />
                  <input>
                     <data path="parent/parent/parent/grtos/data" />
                  </input>
               </function_hash>
            </module_attribute_extension>
            <process_attribute_extension_cdf>
               <number name="extension_type" size="4" value="5" />
               <number name="extension_length" size="4" calculate="parent.size" />
               <number name="process_flags" size="4" value="0" />
               <number name="main_thread_id" size="4" value="0" />
               <number name="private_code_base_address" size="4" calculate="/settings/grtos_elf.private_code_base_address" />
               <number name="uncompressed_private_code_size" size="4" calculate="/settings/grtos_elf.uncompressed_private_code_size" />
               <number name="cm0_heap_size" size="4" value="0" />
               <number name="bss_size" size="4" calculate="/settings/grtos_elf.bss_size" />
               <number name="def_heap_size" size="4" value="0" />
               <number name="main_thread_entry_point" size="4" calculate="/settings/grtos_elf.entry_address" />
               <number name="reserved" size="28" value="0" />
            </process_attribute_extension_cdf>
            <ie_attribute_info>
              <number name="magic" size="4" value="0x464c452e" />
              <number name="me_off" size="4" calculate="parent/parent/parent/grtos.offset" />
              <number name="spi_off" size="4" value="0x1000" />
              <number name="text_offset" size="4" value="0" />
              <number name="text_filsz" size="4" calculate="/settings/grtos_elf.text_filsz" />
              <number name="text_memsz" size="4" calculate="/settings/grtos_elf.text_memsz" />
              <number name="text_paddr" size="4" calculate="/settings/grtos_elf.text_paddr" />
              <number name="text_start" size="4" calculate="/settings/grtos_elf.entry_address" />
              <number name="data_offset" size="4" calculate="parent/parent/parent/grtos/data.offset - parent/parent/parent/grtos.offset" />
              <number name="data_filsz" size="4" calculate="/settings/grtos_elf.data_filsz" />
              <number name="data_memsz" size="4" calculate="/settings/grtos_elf.data_memsz" />
              <number name="data_paddr" size="4" calculate="/settings/grtos_elf.data_paddr" />
            </ie_attribute_info>
         </metadata>
         
         <metadata name="rtos_met">
            <module_attribute_extension>
               <number name="extension_type" size="4" value="10" />
               <number name="extension_length" size="4" calculate="parent.size" />
               <number name="compression_type" size="1" value="0" />
               <number name="encryption_type" size="1" value="0" />
               <number name="mode" size="1" value="0" />
               <number name="reserved" size="1" value="0" />
               <number name="uncompressed_size" size="4" calculate="parent/parent/parent/rtos.size" />
               <number name="compressed_size" size="4" calculate="parent/parent/parent/rtos.size" />
               <number name="global_module_id_process_number" size="2" value="1" />
               <number name="global_module_id_vendor_id" size="2" value="0x8086" />
               <function_hash name="image_hash" enabled="parent/parent/parent/rtos.enabled != 0" >
                  <sha value="256" />
                  <input>
                     <data path="parent/parent/parent/rtos" />
                  </input>
               </function_hash>
               <byte_array name="hash" size="32" value="" enabled="parent/parent/parent/rtos.enabled == 0" />
            </module_attribute_extension>
            <process_attribute_extension_cdf>
               <number name="extension_type" size="4" value="5" />
               <number name="extension_length" size="4" calculate="parent.size" />
               <number name="process_flags" size="4" value="0" />
               <number name="main_thread_id" size="4" value="0" />
               <number name="private_code_base_address" size="4" calculate="parent/parent/parent/rtos.enabled ? /settings/rtos_elf.private_code_base_address : 0" />
               <number name="uncompressed_private_code_size" size="4" calculate="parent/parent/parent/rtos.enabled ? /settings/rtos_elf.uncompressed_private_code_size : 0" />
               <number name="cm0_heap_size" size="4" value="0" />
               <number name="bss_size" size="4" calculate="parent/parent/parent/rtos.enabled ? /settings/rtos_elf.bss_size : 0" />
               <number name="def_heap_size" size="4" value="0" />
               <number name="main_thread_entry_point" size="4" calculate="parent/parent/parent/rtos.enabled ? /settings/rtos_elf.entry_address : 0" />
               <number name="reserved" size="28" value="0" />
            </process_attribute_extension_cdf>
            <ie_attribute_info>
              <number name="magic" size="4" calculate="0x464c452e" />
              <number name="me_off" size="4" calculate="parent/parent/parent/rtos.enabled ? parent/parent/parent/rtos.offset : 0" />
              <number name="spi_off" size="4" calculate="parent/parent/parent/rtos.enabled ? 0x1000 : 0" />
              <number name="text_offset" size="4" calculate="parent/parent/parent/rtos.enabled ? parent/parent/parent/rtos/code.offset - parent/parent/parent/rtos.offset : 0" />
              <number name="text_filsz" size="4" calculate="parent/parent/parent/rtos.enabled ? /settings/rtos_elf.text_filsz : 0" />
              <number name="text_memsz" size="4" calculate="parent/parent/parent/rtos.enabled ? /settings/rtos_elf.text_memsz : 0" />
              <number name="text_paddr" size="4" calculate="parent/parent/parent/rtos.enabled ? /settings/rtos_elf.text_paddr : 0" />
              <number name="text_start" size="4" calculate="parent/parent/parent/rtos.enabled ? parent/parent/parent/rtos/code.offset : 0" />
              <number name="data_offset" size="4" calculate="parent/parent/parent/rtos.enabled ? parent/parent/parent/rtos/data.offset - parent/parent/parent/rtos.offset : 0" />
              <number name="data_filsz" size="4" calculate="parent/parent/parent/rtos.enabled ? /settings/rtos_elf.data_filsz : 0" />
              <number name="data_memsz" size="4" calculate="parent/parent/parent/rtos.enabled ? /settings/rtos_elf.data_memsz : 0" />
              <number name="data_paddr" size="4" calculate="parent/parent/parent/rtos.enabled ? /settings/rtos_elf.data_paddr : 0" />
            </ie_attribute_info>
         </metadata>
         
         <metadata name="data_met" enabled="/settings/data_enabled.value == 1">
            <manifest_header>
               <number name="header_type" size="4" value="4" />
               <number name="header_length" size="4" calculate="parent.size / 4" />
               <bit_field name="header_version" size="4" >
                  <bit name="reserved0" bit_low="0" bit_high="11" value="0" />
                  <bit name="sha_type" bit_low="12" bit_high="15" value="0" />
                  <bit name="key_type" bit_low="16" bit_high="19" calculate="/settings/ftpr_key.signature_size == 256 ? 1 : 2" />
                  <bit name="reserved1" bit_low="20" bit_high="31" value="0" />
               </bit_field>
               <bit_field name="flags" size="4" >
                  <bit name="pv_state" bit_low="0" bit_high="0" value="0" />
                  <bit name="reserved" bit_low="1" bit_high="30" value="0" />
                  <bit name="debug" bit_low="31" bit_high="31" value="0" />
               </bit_field>
               <number name="vendor" size="4" value="0x8086" />
               <byte_array name="date" size="4" calculate="/settings/date.bcd" />
               <number name="size" size="4" calculate="parent/parent.size / 4" />
               <string name="header_id" size="4" value="$MN2" />
               <number name="reserved" size="4" value="0" />
               <number name="version" size="8" value="0" />
               <number name="svn" size="4" value="0" />
               <number name="reserved" size="72" value="0" />
               <number name="modulus_size" size="4" calculate="parent/modulus.size / 4" />
               <number name="exponent_size" size="4" calculate="parent/exponent.size / 4" />
               <byte_array name="modulus" calculate="/settings/ftpr_key.modulus" />
               <byte_array name="exponent" calculate="/settings/ftpr_key.exponent" />
               <function_sign name="signature">
                  <key value="/settings/ftpr_key" />
                  <sha value="256" />
                  <padding_scheme value="v1_5" salt_len=""/>
                  <input>
                     <data path="parent/parent/parent/data" />
                  </input>
               </function_sign>
            </manifest_header>
         </metadata>
         
         <byte_array name="rbe" calculate="/settings/rbe_elf.bin" align="0x1000" />
         
         <grtos enabled="/settings/grtos_enabled.value == 1" align="0x1000">
            <byte_array name="data" calculate="/settings/grtos_elf.data" />
         </grtos>
         
         <rtos enabled="/settings/rtos_enabled.value == 1" align="0x1000">
            <byte_array name="data" calculate="/settings/rtos_elf.data" />
            <byte_array name="code" calculate="/settings/rtos_elf.code" />
         </rtos>
     
          <byte_array enabled="/settings/data_enabled.value == 1" name="data" calculate="/settings/data.data" />
 
      </partition>
      
      <!-- OKM partition -->
      <partition name="okm" align="0x1000">
         <pdt>
            <pdt_header>
               <string name="header_marker" size="4" value="$CPD" />
               <number name="number_of_entries" size="4" calculate="parent/parent/entries.size / parent/parent/entries/pdt_entry.size" />
               <number name="header_version" size="1" value="1" />
               <number name="entry_version" size="1" value="1" />
               <number name="header_length" size="1" value="0x10" />
               <number name="reserved" size="1" value="0" />
               <string name="partition_name" size="4" value="OKM" />
            </pdt_header>
            <entries>
               <pdt_entry>
                  <string name="entry_name" size="12" value="OEMP.man" />
                  <number name="offset" size="4" calculate="parent/parent/parent/parent/manifest.offset - parent/parent/parent/parent.offset" />
                  <number name="length" size="4" calculate="parent/parent/parent/parent/manifest.size" />
                  <number name="reserved" size="4" value="0" />
               </pdt_entry>
            </entries>
         </pdt>
         
         <manifest> <!--  okm manifest-->
            <manifest_header>
               <number name="header_type" size="4" value="4" />
               <number name="header_length" size="4" calculate="parent.size / 4" />
               <bit_field name="header_version" size="4" >
                  <bit name="reserved0" bit_low="0" bit_high="11" value="0" />
                  <bit name="sha_type" bit_low="12" bit_high="15" value="0" />
                  <bit name="key_type" bit_low="16" bit_high="19" calculate="/settings/key.signature_size == 256 ? 1 : 2" />
