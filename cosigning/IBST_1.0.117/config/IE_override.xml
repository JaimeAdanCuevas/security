<ibst_overrides>
   <number name="bin_size" value="0x400000" />
   <number name="ie_size" value="0x180000" />
   <rsa_key name="ftpr_key" value="data/key.pem" />
   <rsa_key name="iefw_key" value="data/key.pem" />
   <rsa_key name="ibb_key" value="data/key_old.pem" />
   <number name="ftpr_svn" value="1" />
   <number name="iefw_svn" value="2" />
   <number name="ibb_svn" value="3" />
   <version name="ftpr_version" value="4.0.0.1" />
   <version name="iefw_version" value="4.0.0.2" />
   <version name="ibb_version" value="4.0.0.3" />
   <elf_file name="rbe_elf" value="data/rbe.elf">
      <module_entry value="ModuleEntry" />
      <objcopy_args value="-O binary --change-section-lma .data=0x4b000" />
   </elf_file>

   <elf_file name="rtos_elf" value="data/rtos.elf">
      <module_entry value="ModuleEntry" />
   </elf_file>
   <number name="rtos_enabled" value="1" />

   <file name="ibb" value="data/ibb.bin"/>
   <file name="ibbconfig" value="data/ibbconfig.bin"/>
   <number name="ibb_enabled" value="1" />

   <file name="data" value="data/data.bin" >
      <offset value="0" />
      <end value="0x1000" />
   </file>
   <number name="data_enabled" value="1" />
   <number name="data_empty" value="1" />
   <number name="data_size" value="0" />

   <!-- If empty, default signing utility is used -->
   <string name="signing_utility" size="1" value=""/>

   <!-- Additional keys specified by OEM -->
   <node name="key_data_oem_keys">
      <key_data>
      <!-- OS Kernel -->
         <byte_array name="usage" size="16" value="0x00000000800000000000000000000000" />
         <number name="reserved" size="16" value="0" />
         <number name="reserved" size="1" value="0" />
         <number name="hash_algorithm" size="1" value="2" />
         <number name="hash_size" size="2" value="32" />
         <byte_array name="hash" size="32" value="0x1405a8a4eb1c8ac251197d85961409ff15fdcd23d325ccdd88d2175cde3b2736" />
      </key_data>
      <key_data>
      <!-- Any other OEM key... -->
         <byte_array name="usage" size="16" value="0x00000000000000000040000000000000" />
         <number name="reserved" size="16" value="0" />
         <number name="reserved" size="1" value="0" />
         <number name="hash_algorithm" size="1" value="2" />
         <number name="hash_size" size="2" value="32" />
         <byte_array name="hash" size="32" value="0x1405a8a4eb1c8ac251197d85961409ff15fdcd23d325ccdd88d2175cde3b2736" />
      </key_data>
   </node>
</ibst_overrides>